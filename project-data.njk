---
layout: layout.njk
permalink: /project/data/
---
{% set theJs %}
    {% include 'project-data.js' %}
{% endset %}
<!-- Page Heading -->
<div class="alert alert-success  d-none" role="alert" id="accountsSuccess">
</div>
<div class="alert alert-danger  d-none" role="alert" id="accountsAlert">
</div>

 <div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Project Data</h1>

    <div class="mb-3">
        <label for="exampleFormControlSelect1">Actions</label><select class="form-control" id="exampleFormControlSelect1">
            <option>Select</option>
            <option>Publish All</option>
            <option>Import Data</option>
            <option>Download Data</option>
            <option>Download Backpages</option>
            <option>Add Record</option>
        </select>
    </div>

</div>
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Currect project Data</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
        <table id="example" class="display" width="100%"></table>
            <table class="table table-bordered" id="projectdatatable" width="100%" cellspacing="0"></table>
        </div>
    </div>
</div>
{% if env.ENVIRONMENT == 'local' %}
    <script type="text/javascript">{{theJs | safe}}</script>
{% else %}
    <script type="text/javascript">{{theJs | jsmin | safe}}</script>
{% endif %}
