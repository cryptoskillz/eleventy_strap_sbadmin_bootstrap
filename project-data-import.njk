---
layout: layout.njk
permalink: /project/data/import/
---
{% set theJs %}
    {% include 'project-data-import.js' %}
{% endset %}
 <div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Project Imported Data</h1>

    <div class="mb-3">
        <label for="pageActionSelect">Actions</label><select class="form-control" id="pageActionSelect">
            <option value="0">Select</option>
            <!--<option>Publish</option>-->
            <option  value="1">Import Data</option>
            <option  value="2">Merge Data</option>
            <option  value="3">Edit Schema</option>
            <option  value="4">Back</option>
        </select>
    </div>

</div>
<div class="card shadow mb-4" id="importeddatatable">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Imported Data</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
        <table id="example" class="display" width="100%"></table>
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0"></table>
        </div>
    </div>
</div>

<div class="modal" tabindex="-1" role="dialog" id="confirmation-import-modal">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Merged data?</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p>If you import this data it will delete all the existing data!</p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary" id="confirmation-modal-import-button">Ok</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
<div class="modal" tabindex="-1" role="dialog" id="import-modal">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Merging data</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p id="mergingmessage"></p>
      </div>
      
    </div>
  </div>
</div>
{% if env.ENVIRONMENT == 'local' %}
    <script type="text/javascript">{{theJs | safe}}</script>
{% else %}
    <script type="text/javascript">{{theJs | jsmin | safe}}</script>
{% endif %}
<script type="text/javascript" src="/assets/js/jszip.min.js"></script>

