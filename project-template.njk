{% set theJs %}
    {% include 'project-template.js' %}
{% endset %}
<!-- Page Heading -->
  <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800" id="project-header">Project Template</h1>
        <a href="/projects/" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm">
        <i class="fas fa-backward fa-sm text-white-50"></i> Back</a>
    </div>
<p class="mb-4">
</p>
<div class="alert alert-success  d-none" role="alert" id="accountsSuccess">
</div>
<div class="alert alert-danger  d-none" role="alert" id="accountsAlert">


</div>
<div class="form-group">
   <!-- <div id="summernote"></div>-->
<div id="projectkeys">

</div>
<textarea class="form-control form-control-user" id="inp-projectemplate"  rows="60" cols="50">
</textarea>
 
</div>
<button class="btn btn-primary btn-user btn-block" id="btn-template">
    Update
</button>

    <!-- include codemirror css/js -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.65.3/codemirror.min.js" integrity="sha512-/8pAp30QGvOa8tNBv7WmWiPFgYGOg2JdVtqI8vK+xZsqWHnNd939v9s+zJHXZcJe5wPD44D66zz+CLTD3KacYA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>


